{
    "name": "innmind/neo4j-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle wraps the innmind neo4j onm library",
    "keywords": ["neo4j", "orm"],
    "homepage": "http://github.com/Innmind/Neo4jBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Innmind/Neo4jBundle/issues"
    },
    "require": {
        "php": "~7.0",
        "psr/log": "~1.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/config": "~3.0",
        "innmind/neo4j-onm": "^2.0",
        "innmind/filesystem": "^1.0",
        "symfony/yaml": "~3.0",
        "innmind/neo4j-dbal": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Neo4jBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Neo4jBundle\\": "tests",
            "Fixtures\\Innmind\\Neo4jBundle\\": "fixtures"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
